{
  "name": "neo4j-simple",
  "version": "0.8.0",
  "description": "Simple nodes, relations, queries",
  "repository": {
    "type": "git",
    "url": "http://github.com/sebinsua/neo4j-simple"
  },
  "keywords": [
    "neo4j",
    "cypher",
    "graph",
    "node",
    "relationship",
    "database",
    "promises",
    "query",
    "simple"
  ],
  "author": "Seb Insua <me@sebinsua.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sebinsua/neo4j-simple/issues"
  },
  "homepage": "https://github.com/sebinsua/neo4j-simple",
  "main": "dist/index.js",
  "babel": {
    "plugins": [
      "ramda",
      "transform-class-properties",
      "transform-export-extensions",
      "transform-flow-strip-types"
    ],
    "presets": [
      "es2015-node"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "babel src --out-dir dist",
    "test": "npm run lint && npm run flow && npm run unit",
    "lint": "standard --verbose src/**/* | snazzy",
    "flow": "flow",
    "unit": "ava --verbose test/**/*",
    "unit:watch": "ava --watch test/**/*"
  },
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "any-promise": "^1.3.0",
    "debug": "^2.2.0",
    "joi": "^9.0.4",
    "neo4j-driver": "^1.1.0-M01",
    "node-emoji": "^1.3.1",
    "node-uuid": "^1.4.7",
    "ramda": "^0.22.1",
    "tap-debug": "^0.1.8",
    "thenify-all": "^1.6.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-ramda": "^1.1.5",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015-node": "^6.1.0",
    "eslint-plugin-flowtype": "^2.11.0",
    "flow-bin": "^0.30.0",
    "snazzy": "^4.0.1",
    "testdouble": "^1.6.0"
  }
}
